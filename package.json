{
  "name": "muscadine-health-ai",
  "version": "0.0.11",
  "description": "Institutional-grade AI chat for health and financial self-sovereignty",
  "scripts": {
    "dev": "concurrently \"vite\" \"ts-node -P tsconfig.server.json src/server.ts\"",
    "dev:frontend": "vite",
    "dev:backend": "ts-node -P tsconfig.server.json src/server.ts",
    "build": "vite build",
    "build:server": "tsc -P tsconfig.server.json",
    "start": "node dist-server/server.js",
    "start:dev": "npm run build && npm run start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "setup": "npm install && npm run db:generate && npm run db:push",
    "test": "vitest run"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/node-fetch": "^2.6.12",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prisma": "^6.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "vite": "^7.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "jsdom": "^26.1.0",
    "sucrase": "^3.35.0",
    "terser": "^5.43.1",
    "ts-interface-checker": "^1.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4"
  }
}
